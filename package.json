{
  "name": "Page.qy",
  "version": "0.0.2",
  "productName": "Page.qy",
  "description": "A simple way to build static website for everyone.",
  "main": "main.js",
  "dependencies": {
    "cheerio": "^0.22.0",
    "download-file": "^0.1.5",
    "extract-zip": "^1.6.5",
    "github-api": "^3.0.0",
    "nedb": "^1.8.0",
    "node-fs-extra": "^0.8.1",
    "request": "^2.81.0",
    "simple-git": "^1.69.0",
    "wangeditor": "^2.1.23"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-lodash": "^3.2.11",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.27.3",
    "electron": "^1.6.2",
    "electron-devtools-installer": "^2.1.0",
    "electron-installer-windows": "^0.2.0",
    "electron-packager": "^8.7.0",
    "electron-prebuilt": "^1.4.13",
    "electron-rebuild": "^1.5.7",
    "react": "^15.4.2",
    "react-addons-pure-render-mixin": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-rte": "^0.11.0",
    "reactcss": "^1.1.1",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.2"
  },
  "scripts": {
    "start": "electron-rebuild && electron --js-flags=\"--harmony-async-await\" main.js",
    "dev": "webpack-dev-server --content-base",
    "build-mac": "electron-packager . Page.qy --platform=darwin --arch=x64 --icon=build/icon.icns --ignore=node_modules --ignore=dist --ignore=.idea --ignore=webpack.config.js --ignore=yarn --ignore=.gitignore  --overwrite --rebuild",
    "build-win": "electron-packager . Page.qy --platform=win32 --arch=x64 --out build --icon=build/icon.ico --ignore=node_modules --ignore=dist --ignore=.idea --ignore=webpack.config.js --ignore=yarn --ignore=.gitignore --overwrite --rebuild",
    "build-linux": "electron-packager . Page.qy --platform=linux --arch=x64 --icon=build/icon.icns --ignore=node_modules --ignore=dist --ignore=.idea --ignore=webpack.config.js --ignore=yarn --ignore=.gitignore  --overwrite --rebuild",
    "setup": "electron-installer-windows --config build/config.json"
  },
  "keywords": [
    "Page",
    "website",
    "blog"
  ],
  "author": "https://github.com/HuQingyang",
  "license": "ISC",
  "ecmaFeatures": {
    "modules": true
  }
}
